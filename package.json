{
  "name": "shower-analysis",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "rm -rf node_modules package-lock.json",
    "clean-data": "ts-node src/scripts/cleanData.ts",
    "inspect": "ts-node src/scripts/inspectArtifacts.ts",
    "filter-videos": "ts-node src/scripts/filterNonBathrooms.ts",
    "format-ar-data": "ts-node src/scripts/formatArData.ts",
    "full-pipeline": "npm run validate && npm run sync && npm run clean-data && npm run filter-videos && npm run format-ar-data && npm run inspect",
    "sync": "ts-node src/scripts/syncArtifacts.ts",
    "validate": "ts-node src/scripts/validateArtifacts.ts",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:sh && ./scripts/check-eslint-disable.sh && npm run check-format",
    "lint:ts": "eslint . --ext .ts,.tsx,.js,.cjs",
    "lint:sh": "npx shellcheck scripts/*.sh",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.cjs --fix && markdownlint . --fix",
    "lint:md": "markdownlint .",
    "format": "npm run format:ts && npm run format:md && npm run format:json && npm run format:css && npm run format:sh",
    "format:ts": "prettier --write \"**/*.{ts,tsx,js,cjs}\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:json": "prettier --write \"**/*.json\"",
    "format:css": "prettier --write \"**/*.css\"",
    "format:sh": "prettier --write \"scripts/*.sh\"",
    "check-format": "prettier --check .",
    "pre-commit": "npm run format && npm run lint && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bondchris/shower-analysis.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/bondchris/shower-analysis/issues"
  },
  "homepage": "https://github.com/bondchris/shower-analysis#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/cli-progress": "^3.11.6",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "cli-progress": "^3.12.0",
    "convert-units": "^2.3.4",
    "dotenv": "^17.2.3",
    "fluent-ffmpeg": "^2.1.3",
    "lodash": "^4.17.21",
    "playwright": "^1.57.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/convert-units": "^2.3.12",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^30.2.0",
    "markdownlint-cli": "^0.46.0",
    "prettier": "^3.7.4",
    "prettier-plugin-sh": "^0.18.0",
    "shellcheck": "^4.1.0",
    "shfmt": "^0.0.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2"
  }
}
