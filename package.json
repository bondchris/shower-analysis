{
  "name": "shower-analysis",
  "version": "0.51.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css && npm run build:ts",
    "build:css": "npx @tailwindcss/cli -i ./src/templates/styles/input.css -o ./src/templates/styles/print.css",
    "build:ts": "tsc",
    "check-format": "prettier --check .",
    "clean": "rm -rf node_modules package-lock.json coverage dist",
    "clear-cache": "find data/artifacts -name 'rawScanMetadata.json' -type f -delete",
    "clean-data": "ts-node src/scripts/cleanData.ts",
    "filter-videos": "ts-node src/scripts/filterNonBathrooms.ts",
    "format": "npm run format:ts && npm run format:md && npm run format:json && npm run format:css && npm run format:sh",
    "format-data": "ts-node src/scripts/formatData.ts",
    "format:css": "prettier --write \"**/*.css\"",
    "format:json": "prettier --write \"**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:sh": "prettier --write \"scripts/*.sh\"",
    "format:ts": "prettier --write \"**/*.{ts,tsx,js,cjs,mjs}\"",
    "full-pipeline": "npm run validate && npm run sync && npm run clean-data && npm run filter-videos && npm run format-data && npm run inspect",
    "inspect": "ts-node src/scripts/inspectArtifacts.ts",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:sh && ./scripts/check-eslint-disable.sh && npm run check-format",
    "lint:fix": "eslint . --fix && markdownlint . --fix",
    "lint:md": "markdownlint .",
    "lint:sh": "npx shellcheck scripts/*.sh",
    "lint:ts": "eslint .",
    "pre-commit": "npm run format && npm run lint && npm run build && npm run test:coverage",
    "sync": "ts-node src/scripts/syncArtifacts.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "validate": "ts-node src/scripts/validateArtifacts.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bondchris/shower-analysis.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/bondchris/shower-analysis/issues"
  },
  "homepage": "https://github.com/bondchris/shower-analysis#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@noble/hashes": "^2.0.1",
    "@types/cli-progress": "^3.11.6",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@visx/axis": "4.0.1-alpha.0",
    "@visx/gradient": "4.0.1-alpha.0",
    "@visx/grid": "4.0.1-alpha.0",
    "@visx/group": "4.0.1-alpha.0",
    "@visx/scale": "4.0.1-alpha.0",
    "@visx/shape": "4.0.1-alpha.0",
    "@visx/text": "4.0.1-alpha.0",
    "axios": "^1.13.2",
    "chroma-js": "^3.2.0",
    "cli-progress": "^3.12.0",
    "convert-units": "^2.3.4",
    "dotenv": "^17.2.3",
    "fluent-ffmpeg": "^2.1.3",
    "lodash": "^4.17.21",
    "mathjs": "^15.1.0",
    "opentype.js": "^1.3.4",
    "playwright": "^1.57.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/chroma-js": "^3.1.2",
    "@types/convert-units": "^2.3.12",
    "@types/lodash": "^4.17.21",
    "@types/node": "^25.0.2",
    "@types/opentype.js": "^1.3.8",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "markdownlint-cli": "^0.47.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-sh": "^0.18.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "shellcheck": "^4.1.0",
    "shfmt": "^0.0.1",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  }
}
